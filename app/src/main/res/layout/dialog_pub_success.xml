<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="@dimen/qb_px_14"
    android:paddingRight="@dimen/qb_px_14">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/qb_px_293"
        android:background="@drawable/pub_dialog_bg">

        <ImageView
            android:id="@+id/pub_success_close"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="@dimen/qb_px_25"
            android:layout_marginTop="@dimen/qb_px_18"
            android:padding="@dimen/qb_px_5"
            android:src="@mipmap/dialog_close" />

        <com.eggplant.qiezisocial.widget.QzTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/qb_px_32"
            android:text="发布成功"
            android:textColor="@color/tv_black2"
            android:textSize="20sp"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@mipmap/pub_success_bg" />

        <com.eggplant.qiezisocial.widget.QzTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|bottom"
            android:text="正在匹配最适合你的人"
            android:textColor="@color/tv_black2"
            android:layout_marginBottom="@dimen/qb_px_47"
            android:textSize="15sp" />
    </FrameLayout>

</FrameLayout>